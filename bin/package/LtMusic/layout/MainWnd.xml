<!--XML最好存储为UTF-8编码-->
<xlue>
	<animation_def class="time.ani">
         <method_def>
            <Action>
            	local arg = {...}
                local self = arg[1]
                local attr = self:GetAttribute()
                local runningTime = self:GetRuningTime()
                if runningTime > 950 then
                	attr.func(self);
                end
            </Action>
         </method_def>
    </animation_def> 
   <animationtemplate id="def.ani" class="time.ani">
             <attr>
                  <totaltime>1000</totaltime>
                  <loop>true</loop> 
             </attr>
  </animationtemplate>
